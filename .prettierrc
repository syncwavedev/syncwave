{
    "bracketSpacing": false,
    "arrowParens": "avoid",

    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-organize-imports"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
