{
    "name": "syncwave-data",
    "private": true,
    "version": "0.1.0",
    "main": "dist/cjs/src/index.js",
    "module": "dist/esm/src/index.js",
    "types": "dist/esm/src/index.d.ts",
    "engines": {
        "node": ">=20.0.0"
    },
    "files": [
        "dist/esm/src",
        "dist/esm/package.json",
        "dist/cjs/src",
        "dist/cjs/package.json",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/src/index.js",
            "require": "./dist/cjs/src/index.js",
            "default": "./dist/cjs/src/index.js"
        }
    },
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "dev": "run-p dev:*",
        "dev:js": "tsc -w",
        "dev:dart": "tsx --watch scripts/gen-dart-participant-client.ts | pino-pretty --config ../../.pino-prettyrc",
        "deploy": "cp ../../README.md ./ && tsx ../../scripts/deploy.ts && rm ./README.md",
        "test": "vitest run"
    },
    "devDependencies": {
        "@types/bytewise": "^1.1.2",
        "@types/deep-equal": "^1.0.4",
        "@types/functional-red-black-tree": "^1.0.6",
        "@types/seedrandom": "^3.0.8",
        "npm-run-all": "^4.1.5",
        "pino-pretty": "^13.0.0",
        "quicktype": "^23.0.171",
        "quicktype-core": "^23.0.171",
        "rimraf": "^6.0.1",
        "tsx": "^4.17.0",
        "typedoc": "^0.26.5",
        "vitest": "^1.6.0",
        "zod-to-json-schema": "^3.24.1"
    },
    "dependencies": {
        "@webfill/async-context": "^1.0.0",
        "async-channel": "^0.2.0",
        "bytewise": "^1.1.0",
        "deep-equal": "^2.2.3",
        "functional-red-black-tree": "^1.0.1",
        "ix": "^7.0.0",
        "lru-cache": "^11.0.2",
        "msgpackr": "^1.11.2",
        "nanoid": "^5.0.9",
        "pino": "^9.6.0",
        "seedrandom": "^3.0.5",
        "typescript": "^5.7.0",
        "uninstall": "^0.0.0",
        "uuid": "^11.0.3",
        "yjs": "^13.6.20",
        "zod": "^3.24.1"
    }
}
