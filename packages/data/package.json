{
    "name": "syncwave-data",
    "private": true,
    "version": "0.1.0",
    "main": "dist/cjs/src/index.js",
    "module": "dist/esm/src/index.js",
    "types": "dist/esm/src/index.d.ts",
    "engines": {
        "node": ">=22.0.0"
    },
    "files": [
        "dist/esm/src",
        "dist/esm/package.json",
        "dist/cjs/src",
        "dist/cjs/package.json",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/src/index.js",
            "require": "./dist/cjs/src/index.js",
            "default": "./dist/cjs/src/index.js"
        },
        "./telemetry.js": {
            "import": "./dist/esm/src/telemetry.js",
            "require": "./dist/cjs/src/telemetry.js",
            "default": "./dist/cjs/src/telemetry.js"
        }
    },
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "dev": "npm run js",
        "js": "tsc -w --preserveWatchOutput",
        "dt": "tsx watch --clear-screen=false scripts/gen-dart-participant-client.ts | pino-pretty --config ../../.pino-prettyrc",
        "deploy": "cp ../../README.md ./ && tsx ../../scripts/deploy.ts && rm ./README.md",
        "test": "vitest run",
        "stress": "run-s stress:*",
        "stress:mvcc-adapter": "NODE_ENV=test tsx ./stress/rw-mvcc-adapter.ts ci"
    },
    "devDependencies": {
        "@types/bytewise": "^1.1.2",
        "@types/deep-equal": "^1.0.4",
        "@types/functional-red-black-tree": "^1.0.6",
        "@types/seedrandom": "^3.0.8",
        "gts": "^6.0.2",
        "npm-run-all": "^4.1.5",
        "pino-pretty": "^13.0.0",
        "quicktype": "^23.0.171",
        "quicktype-core": "^23.0.171",
        "rimraf": "^6.0.1",
        "tsx": "^4.17.0",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.57.1",
        "@opentelemetry/resources": "^1.30.1",
        "@opentelemetry/sdk-trace-base": "^1.30.1",
        "@opentelemetry/semantic-conventions": "^1.30.0",
        "@sinclair/typebox": "^0.34.28",
        "@webfill/async-context": "^1.0.0",
        "async-channel": "^0.2.0",
        "deep-equal": "^2.2.3",
        "fdb-tuple": "^1.0.0",
        "functional-red-black-tree": "^1.0.1",
        "ix": "^7.0.0",
        "lru-cache": "^11.0.2",
        "msgpackr": "^1.11.2",
        "nanoid": "^5.0.9",
        "pino": "^9.6.0",
        "seedrandom": "^3.0.5",
        "typescript": "^5.8.0",
        "uuid": "^11.0.3",
        "yjs": "^13.6.20"
    }
}
