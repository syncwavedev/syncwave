{
    "name": "syncwave",
    "version": "0.1.0",
    "main": "dist/cjs/src/index.js",
    "module": "dist/esm/src/index.js",
    "types": "dist/esm/src/index.d.ts",
    "engines": {
        "node": ">=22.0.0"
    },
    "files": [
        "dist/esm/src",
        "dist/esm/package.json",
        "dist/cjs/src",
        "dist/cjs/package.json",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/src/index.js",
            "require": "./dist/cjs/src/index.js",
            "default": "./dist/cjs/src/index.js"
        },
        "./telemetry.js": {
            "import": "./dist/esm/src/telemetry.js",
            "require": "./dist/cjs/src/telemetry.js",
            "default": "./dist/cjs/src/telemetry.js"
        },
        "./node-crypto-provider.js": {
            "import": "./dist/esm/src/node-crypto-provider.js",
            "require": "./dist/cjs/src/node-crypto-provider.js",
            "default": "./dist/cjs/src/node-crypto-provider.js"
        },
        "./node-jwt-provider.js": {
            "import": "./dist/esm/src/node-jwt-provider.js",
            "require": "./dist/cjs/src/node-jwt-provider.js",
            "default": "./dist/cjs/src/node-jwt-provider.js"
        },
        "./web-crypto-provider.js": {
            "import": "./dist/esm/src/web-crypto-provider.js",
            "require": "./dist/cjs/src/web-crypto-provider.js",
            "default": "./dist/cjs/src/web-crypto-provider.js"
        }
    },
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "dev": "tsc -w --preserveWatchOutput",
        "deploy": "cp ../../README.md ./ && tsx ../../scripts/deploy.ts && rm ./README.md",
        "test": "vitest run",
        "stress": "run-s stress:*",
        "stress:mvcc-adapter": "NODE_ENV=test tsx ./stress/rw-mvcc-adapter.ts ci"
    },
    "devDependencies": {
        "@types/functional-red-black-tree": "^1.0.6",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/seedrandom": "^3.0.8",
        "npm-run-all": "^4.1.5",
        "pino-pretty": "^13.0.0",
        "rimraf": "^6.0.1",
        "tsx": "^4.17.0",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/api-logs": "^0.200.0",
        "@opentelemetry/resources": "^2.0.0",
        "@opentelemetry/sdk-trace-base": "^2.0.0",
        "@opentelemetry/semantic-conventions": "^1.30.0",
        "@sinclair/typebox": "^0.34.28",
        "@webfill/async-context": "^1.0.0",
        "async-channel": "^0.2.0",
        "fdb-tuple": "^1.0.0",
        "functional-red-black-tree": "^1.0.1",
        "ix": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "syncwave-config": "*",
        "lib0": "^0.2.99",
        "lru-cache": "^11.0.2",
        "msgpackr": "^1.11.2",
        "seedrandom": "^3.0.5",
        "typescript": "^5.8.0",
        "uuid": "^11.0.3",
        "yjs": "^13.6.20"
    }
}
