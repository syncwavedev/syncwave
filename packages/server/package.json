        "@types/koa-static": "^4.0.4",
        "@types/node": "^22.10.2",
        "@types/ws": "^8.5.13",
        "pino-pretty": "^13.0.0",
        "rimraf": "^6.0.1",
        "tsx": "^4.17.0",
        "vitest": "^1.6.0"
    },
    "dependencies": {
